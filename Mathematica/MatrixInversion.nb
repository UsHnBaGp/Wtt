         "]"}], ";", " ", 
        RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\n", "\n", 
     RowBox[{"(*", 
      RowBox[{"Remove", " ", "helper", " ", "function", " ", "KiraWeight"}], 
      "*)"}], "\n", 
     RowBox[{"KiraWeights", "=", 
      RowBox[{"KiraWeights", "[", 
       RowBox[{"[", 
        RowBox[{";;", ",", "1"}], "]"}], "]"}]}], ";", "\n", 
     RowBox[{"(*", 
      RowBox[{"Remove", " ", "0", " ", "coefficients"}], "*)"}], "\n", 
     RowBox[{"WeightCoef", "=", 
      RowBox[{"WeightCoef", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\n", "\n", 
     RowBox[{"(*", 
      RowBox[{"Convert", " ", "to", " ", "Kira", " ", 
       RowBox[{"string", ":"}]}], " ", "*)"}], "\n", 
