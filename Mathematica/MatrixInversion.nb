          RowBox[{"MapThread", "[", 
           RowBox[{"CoefficientToRatracer", ",", 
            RowBox[{"{", 
             RowBox[{"WeightCoef", ",", "KiraWeights"}], "}"}]}], "]"}]}]}], 
        ";", "\n", 
        RowBox[{"Return", "[", "RatRacerRelation", "]"}]}]}], "\n", "]"}], 
     ";", "\n", "\n", 
     RowBox[{"KiraRelation", "=", 
      RowBox[{"StringJoin", "@", 
